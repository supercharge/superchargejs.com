{
  "name": "supercharge-homepage",
  "description": "Supercharge homepage",
  "version": "2.0.0",
  "dependencies": {
    "@supercharge/collections": "~4.3.0",
    "@supercharge/console": "~2.0.0-alpha.9",
    "@supercharge/contracts": "~2.0.0-alpha.9",
    "@supercharge/core": "~2.0.0-alpha.9",
    "@supercharge/facades": "~2.0.0-alpha.9",
    "@supercharge/fs": "~3.4.0",
    "@supercharge/goodies": "~1.10.1",
    "@supercharge/http": "~2.0.0-alpha.9",
    "@supercharge/routing": "~2.0.0-alpha.7",
    "@supercharge/strings": "~1.28.0",
    "@supercharge/view": "~2.0.0-alpha.9",
    "front-matter": "~4.0.2",
    "marked": "~4.0.17",
    "shiki": "~0.10.1"
  },
  "devDependencies": {
    "@supercharge/eslint-config-typescript": "~2.2.0",
    "@supercharge/tsconfig": "~3.1.0",
    "@swc-node/register": "~1.5.1",
    "@tailwindcss/forms": "~0.5.2",
    "@tailwindcss/typography": "~0.5.2",
    "@types/marked": "~4.0.3",
    "@types/node": "~17.0.42",
    "@types/supertest": "~2.0.12",
    "autoprefixer": "~10.4.7",
    "c8": "~7.11.3",
    "concurrently": "~7.2.1",
    "cssnano": "~5.1.11",
    "eslint": "~8.17.0",
    "expect": "~28.1.1",
    "nodemon": "~2.0.16",
    "postcss": "~8.4.14",
    "postcss-cli": "~9.1.0",
    "supertest": "~6.2.3",
    "tailwindcss": "~3.1.2",
    "tailwindcss-textshadow": "~2.1.3",
    "ts-node": "~10.8.1",
    "typescript": "~4.7.3",
    "uvu": "~0.5.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node -r @swc-node/register server.ts",
    "dev": "concurrently \"npm run dev:tsc:check\" \"npm run dev:css\" \"npm run dev:server\"",
    "dev:css": "npm run build:css -- --watch",
    "build:css": "postcss resources/css/app.css -o public/css/app.css",
    "dev:server": "nodemon --ext ts,js,hbs --exec npm start",
    "prod": "npm run production",
    "production": "concurrently \"npm run prod:css\"",
    "prod:css": "NODE_ENV=production postcss resources/css/app.css -o public/css/app.css",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "tsc:check": "tsc --noEmit --skipLibCheck",
    "dev:tsc:check": "npm run tsc:check -- --watch",
    "test": "npm run lint && npm run test:run",
    "test:run": "c8 --include=dist uvu -r @swc-node/register",
    "posttest": "c8 report --reporter=html"
  }
}
