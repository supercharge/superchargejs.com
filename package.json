{
  "name": "supercharge-homepage",
  "description": "Supercharge homepage",
  "version": "2.0.0",
  "dependencies": {
    "@supercharge/collections": "~4.1.0",
    "@supercharge/console": "~2.0.0-alpha.8",
    "@supercharge/contracts": "~2.0.0-alpha.8",
    "@supercharge/core": "~2.0.0-alpha.8",
    "@supercharge/facades": "~2.0.0-alpha.8",
    "@supercharge/fs": "~3.4.0",
    "@supercharge/goodies": "~1.10.0",
    "@supercharge/http": "~2.0.0-alpha.8",
    "@supercharge/routing": "~2.0.0-alpha.7",
    "@supercharge/strings": "~1.24.0",
    "@supercharge/view": "~2.0.0-alpha.8",
    "front-matter": "~4.0.2",
    "marked": "~3.0.7",
    "shiki": "~0.10.0"
  },
  "devDependencies": {
    "@supercharge/eslint-config-typescript": "~1.0.1",
    "@supercharge/tsconfig": "~3.1.0",
    "@tailwindcss/forms": "~0.4.0",
    "@tailwindcss/typography": "~0.5.0",
    "@types/marked": "~3.0.2",
    "@types/node": "~17.0.8",
    "@types/supertest": "~2.0.11",
    "autoprefixer": "~10.4.2",
    "c8": "~7.11.0",
    "concurrently": "~7.0.0",
    "cssnano": "~5.0.15",
    "eslint": "~7.32.0",
    "expect": "~27.4.6",
    "nodemon": "~2.0.15",
    "postcss": "~8.4.5",
    "postcss-cli": "~9.1.0",
    "supertest": "~6.2.1",
    "tailwindcss": "~3.0.15",
    "tailwindcss-textshadow": "~2.1.3",
    "ts-eager": "~2.0.2",
    "ts-node": "~10.4.0",
    "typescript": "~4.4.4",
    "uvu": "~0.5.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-eager server.ts",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:server\"",
    "dev:css": "npm run build:css -- --watch",
    "build:css": "postcss resources/css/app.css -o public/css/app.css",
    "dev:server": "nodemon --ext ts,js,hbs --exec 'ts-eager' server.ts",
    "prod": "npm run production",
    "production": "concurrently \"npm run prod:css\"",
    "prod:css": "NODE_ENV=production postcss resources/css/app.css -o public/css/app.css",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:coverage",
    "test:run": "uvu -r ts-eager/register",
    "test:coverage": "c8 --include=dist npm run test:run && npm run test:report",
    "test:report": "c8 report --reporter=html"
  }
}
