{
  "name": "supercharge-homepage",
  "description": "Supercharge homepage",
  "version": "2.0.0",
  "dependencies": {
    "@supercharge/console": "~2.0.0-alpha.5",
    "@supercharge/contracts": "~2.0.0-alpha.5",
    "@supercharge/core": "~2.0.0-alpha.5",
    "@supercharge/facades": "~2.0.0-alpha.5",
    "@supercharge/fs": "~3.4.0",
    "@supercharge/http": "~2.0.0-alpha.5",
    "@supercharge/routing": "~2.0.0-alpha.5",
    "@supercharge/strings": "~1.21.0",
    "@supercharge/view": "~2.0.0-alpha.5",
    "marked": "~3.0.4",
    "shiki": "~0.9.11"
  },
  "devDependencies": {
    "@supercharge/tsconfig": "~3.1.0",
    "@tailwindcss/custom-forms": "~0.2.1",
    "@tailwindcss/typography": "~0.4.1",
    "@tailwindcss/ui": "~0.7.2",
    "@types/marked": "~3.0.1",
    "@types/node": "~16.10.1",
    "@typescript-eslint/eslint-plugin": "~4.32.0",
    "autoprefixer": "~10.3.6",
    "concurrently": "~6.2.2",
    "eslint": "~7.32.0",
    "eslint-config-standard": "~16.0.3",
    "eslint-config-standard-with-typescript": "~21.0.1",
    "eslint-plugin-import": "~2.24.2",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~5.1.0",
    "eslint-plugin-standard": "~4.1.0",
    "nodemon": "~2.0.13",
    "postcss": "~8.3.8",
    "postcss-cli": "~9.0.1",
    "tailwindcss": "~2.2.16",
    "tailwindcss-textshadow": "~2.1.3",
    "ts-node": "~10.2.1",
    "typescript": "~4.4.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:server\"",
    "dev:css": "postcss --watch resources/css/app.css -o public/css/app.css",
    "dev:server": "nodemon --ext ts,js,hbs --exec 'ts-node' server.ts",
    "prod": "npm run production",
    "production": "concurrently \"npm run prod:css\"",
    "prod:css": "NODE_ENV=production postcss resources/css/app.css -o public/css/app.css",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo 'Missing tests.' && exit 1"
  }
}
