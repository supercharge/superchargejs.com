{
  "name": "supercharge-homepage",
  "description": "Supercharge homepage",
  "version": "2.0.0",
  "dependencies": {
    "@supercharge/console": "~2.0.0-alpha.3",
    "@supercharge/contracts": "~2.0.0-alpha.3",
    "@supercharge/core": "~2.0.0-alpha.4",
    "@supercharge/facades": "~2.0.0-alpha.3",
    "@supercharge/fs": "~3.3.0",
    "@supercharge/http": "~2.0.0-alpha.3",
    "@supercharge/routing": "~2.0.0-alpha.3",
    "@supercharge/strings": "~1.19.0",
    "@supercharge/view": "~2.0.0-alpha.4",
    "marked": "~2.1.3",
    "shiki": "~0.9.6"
  },
  "devDependencies": {
    "@supercharge/tsconfig": "~3.0.0",
    "@tailwindcss/custom-forms": "~0.2.1",
    "@tailwindcss/typography": "~0.4.1",
    "@tailwindcss/ui": "~0.7.2",
    "@types/marked": "~2.0.4",
    "@types/node": "~16.6.1",
    "@typescript-eslint/eslint-plugin": "~4.29.1",
    "autoprefixer": "~10.3.1",
    "concurrently": "~6.2.1",
    "eslint": "~7.32.0",
    "eslint-config-standard": "~16.0.3",
    "eslint-config-standard-with-typescript": "~20.0.0",
    "eslint-plugin-import": "~2.24.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~5.1.0",
    "eslint-plugin-standard": "~4.1.0",
    "nodemon": "~2.0.12",
    "postcss": "~8.3.6",
    "postcss-cli": "~8.3.1",
    "tailwindcss": "~2.2.7",
    "tailwindcss-textshadow": "~2.1.3",
    "ts-node": "~10.2.0",
    "typescript": "~4.3.5"
  },
  "engines": {
    "node": ">= 12"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "build:css": "postcss resources/css/app.css -o public/css/app.css",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:server\"",
    "dev:css": "postcss --watch resources/css/app.css -o public/css/app.css",
    "dev:server": "nodemon --ext ts,js,hbs --exec 'ts-node' server.ts",
    "start": "ts-node server.ts",
    "test": "echo 'Missing tests.' && exit 1"
  }
}
